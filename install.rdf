	<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	  <Description about="urn:mozilla:install-manifest">
		<em:id>MouseControl@unlisted</em:id>
		<em:version>2.0</em:version>
		<em:type>2</em:type>
		<em:bootstrap>true</em:bootstrap>
		<em:multiprocessCompatible>true</em:multiprocessCompatible>
		<em:optionsType>3</em:optionsType>
		<em:optionsURL>about:mousecontrol</em:optionsURL>

		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>33.0</em:minVersion> <!-- due to e10s -->
				<em:maxVersion>48.0</em:maxVersion> <!-- the latest version i tested it in -->
			</Description>
		</em:targetApplication>

		<!-- Front End MetaData -->
		<!-- must provide default non-localized because It's used as a default on AMO. It's used as a default by the add-on manager, with the possibility of other locales overriding it. Failure to provide a non-localized name will lead to failed upload on AMO. -->
		<em:name>MouseControl</em:name>
		<em:description>Enables user-defined "natural mouse combinations" to trigger customizable functions.</em:description>
		<em:creator>Noitidart</em:creator>
		<em:contributor>David Vignoni for Icon</em:contributor>
		<em:contributor>Jochen for C API Sourcecode</em:contributor>

		<!-- start localizing - For each entry in default data MUST have equivalent in each localized block -->
		<em:localized>
			<Description>
				<em:locale>en-US</em:locale>
				<em:name>MouseControl</em:name>
				<em:description>Enables user-defined "natural mouse combinations" to trigger customizable functions.</em:description>
				<em:creator>Noitidart</em:creator>
				<em:contributor>David Vignoni for Icon</em:contributor>
				<em:contributor>Jochen for C API Sourcecode</em:contributor>
			</Description>
		</em:localized>
	  </Description>
	</RDF>
